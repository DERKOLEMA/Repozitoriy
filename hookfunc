local acs = game.ReplicatedStorage:FindFirstChild("ACS_Engine")
if not acs then
    return
end

local events
for i, v in pairs(acs:GetChildren()) do
    if v:IsA("Folder") then
        local idx = 0
        for ii, vv in pairs(v:GetChildren()) do
            if vv:IsA("RemoteEvent") then
                idx += 1
            end
        end
        if idx >= 15 then
            events = v
            break
        end
    end
end
if not events then
    return
end

local conn
for i, v in pairs(getconnections(events.ServerBullet.OnClientEvent)) do
    conn = v.Function
    break
end
if not conn then
    return
end

local old
old = hookfunction(conn, function(...)
    local args = {...}
    if #args <= 1 then
        return
    end
    return old(...)
end)
